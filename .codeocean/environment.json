{
	"version": 1,
	"base_image": "codeocean/c1-vscode:4.20.0-pylint2023.10.1-copilot1.143.601",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "build-essential",
					"version": "12.9ubuntu3"
				}
			],
			"install_software_properties_common": true
		},
		"pip3": {
			"packages": [
				{
					"name": "aind-data-schema-models",
					"version": "0.5.7"
				},
				{
					"name": "git+https://github.com/AllenInstitute/dynamic_routing_analysis#egg=dynamic-routing-analysis",
					"version": "e1a5608c60a03e74da98807f4185fb2a9b21f40c"
				},
				{
					"name": "lazynwb",
					"version": "0.2.39"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	},
	"env_variables": [
		{
			"name": "POLARS_MAX_THREADS",
			"value": "1"
		},
		{
			"name": "TOKIO_WORKER_THREADS",
			"value": "1"
		},
		{
			"name": "OPENBLAS_NUM_THREADS",
			"value": "1"
		},
		{
			"name": "RAYON_NUM_THREADS",
			"value": "1"
		},
		{
			"name": "OMP_NUM_THREADS",
			"value": "1"
		},
		{
			"name": "MKL_NUM_THREADS",
			"value": "1"
		},
		{
			"name": "NUMEXPR_NUM_THREADS",
			"value": "1"
		},
		{
			"name": "VECLIB_MAXIMUM_THREADS",
			"value": "1"
		}
	]
}
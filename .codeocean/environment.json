{
	"version": 1,
	"base_image": "codeocean/mambaforge3:23.1.0-4-python3.10.12-ubuntu22.04",
	"args": [
		"AWS_ACCESS_KEY_ID",
		"AWS_DEFAULT_REGION",
		"AWS_SECRET_ACCESS_KEY"
	],
	"options": {
		"registry_host_arg": true,
		"git_ask_pass": true
	},
	"installers": {
		"apt": {
			"packages": [
				{
					"name": "build-essential",
					"version": "12.9ubuntu3"
				}
			],
			"install_software_properties_common": true
		},
		"pip3": {
			"packages": [
				{
					"name": "aind-data-schema-models",
					"version": "0.5.7"
				},
				{
					"name": "code-ocean-aux-tools",
					"version": "1.1.3"
				},
				{
					"name": "git+https://github.com/AllenInstitute/dynamic_routing_analysis#egg=dynamic-routing-analysis",
					"version": "10a3c8e46c850c4c1bd70515168d7d3c3c35449c"
				},
				{
					"name": "hdmf",
					"version": "3.14.6"
				},
				{
					"name": "pynwb",
					"version": "3.0.0"
				},
				{
					"name": "scikit-learn",
					"version": "1.5.2"
				}
			],
			"options": {},
			"pre_install_options": {}
		}
	}
}